string input = "";
yap(input.len());
int largest=0;
int length = 1;
string prev = input[0];
for(int i=1;i<input.len();i=i+1){
    if(input[i]==prev){
        length = length + 1;
        if(length > largest){
            largest = length;
        }
    }else{
        length = 1;
    }
    prev = input[i];
}
yap(largest);